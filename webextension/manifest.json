{
  "name": "Send to Kodi 2",
  "version": "3.1.4",
  "applications": {
    "gecko": {
      "id": "{d5ce3231-24c3-4126-bfc8-3e03bcdcb991}"
    }
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "notifications",
    "menus",
    "webRequest",
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "48": "data/img/kodi_logo_only.svg",
    "96": "data/img/kodi_logo_only.svg"
  },
  "background": {
    "scripts": ["main.js"]
  },
  "options_ui": {
    "page": "data/preferences.html",
    "open_in_tab": true,
    "browser_style": false
  },
  "page_action": {
    "browser_style": true,
    "default_icon": "data/img/kodi_logo_only.svg",
    "default_title": "Send to Kodi"
  }
}
